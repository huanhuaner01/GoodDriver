<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- 有先后顺序  即最后的是才是默认的 -->
    <item android:state_pressed="true" android:drawable="@drawable/rect_pre"></item>
    <item><shape>

             <stroke android:width="1dp" android:color="@color/wegroup_item_line_color" />
